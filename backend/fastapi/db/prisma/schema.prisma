generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = "5"
}

datasource db { 
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BlankOrderIdTransaction {
  id               Int            @id @default(autoincrement())
  Order_Id         String?
  Transaction_Type String?
  Payment_Type     String?
  Invoice_Amt      String?
  Net_Amt          String?
  P_Description    String?
  Order_Date       String?
  Payment_Date     String?
  Source           String?
  uploadSessionId  Int?
  UploadSession    UploadSession? @relation(fields: [uploadSessionId], references: [id])
}

model BlankTransactionSummary {
  id              Int            @id @default(autoincrement())
  P_Description   String
  SumNetAmt       String
  uploadSessionId Int?
  UploadSession   UploadSession? @relation(fields: [uploadSessionId], references: [id])
}

model CategorizedTransaction {
  id                   Int            @id @default(autoincrement())
  Order_Id             String?
  Payment_Invoice_Amt  String?
  Return_Invoice_Amt   String?
  Shipment_Invoice_Amt String?
  Payment_Net_Amt      String?
  Return_Net_Amt       String?
  Shipment_Net_Amt     String?
  Category             String
  uploadSessionId      Int?
  UploadSession        UploadSession? @relation(fields: [uploadSessionId], references: [id])
}

model TransactionSummary {
  id              Int            @id @default(autoincrement())
  Category        String
  Count           Int
  uploadSessionId Int?
  UploadSession   UploadSession? @relation(fields: [uploadSessionId], references: [id])
}

model UploadSession {
  id                        Int                       @id @default(autoincrement())
  BlankOrderIdTransactions  BlankOrderIdTransaction[]
  BlankTransactionSummaries BlankTransactionSummary[]
  CategorizedTransactions   CategorizedTransaction[]
  TransactionSummaries      TransactionSummary[]
}
